{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/marcador.io/src",
  "monorepo": true,
  "root": "apps/marcador.io",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/marcador.io/tsconfig.app.json"
  },
  "projects": {
    "marcador.io": {
      "type": "application",
      "root": "apps/marcador.io",
      "entryFile": "main",
      "sourceRoot": "apps/marcador.io/src",
      "compilerOptions": {
        "tsConfigPath": "apps/marcador.io/tsconfig.app.json"
      }
    },
    "marcador-processor": {
      "type": "application",
      "root": "apps/marcador-processor",
      "entryFile": "main",
      "sourceRoot": "apps/marcador-processor/src",
      "compilerOptions": {
        "tsConfigPath": "apps/marcador-processor/tsconfig.app.json"
      }
    }
  }
}